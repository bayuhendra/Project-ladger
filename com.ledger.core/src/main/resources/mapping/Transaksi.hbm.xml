<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class mutable="true" name="com.ledger.domain.financial.management.Transaksi" table="TXN_TRANSAKSI">
        <!--<cache usage="read-only"/>-->
        <id column="id" name="id">
            <generator class="org.hibernate.id.IdentityGenerator"/>
            <!--<generator class="increment"/>-->
        </id>
        <property column="transaksiID" name="transaksiID" not-null="false"/>
        <property column="userID" name="userID" not-null="false"/>
        <property column="namaTransaksi" name="namaTransaksi" not-null="false"/>
        <property column="jenisTransaksi" name="jenisTransaksi" not-null="false"/>
        <property column="nilaiTransaksi" name="nilaiTransaksi" not-null="false"/>
        <property column="cash" name="cash" not-null="false"/>
        <property column="kredit" name="kredit" not-null="false"/>
        <property column="tanggalTransaksi" name="tanggalTransaksi" not-null="false"/>
        <property column="jenisPembayaran" name="jenisPembayaran" not-null="false"/>
        <property column="createdBy" name="createdBy" not-null="false"/>
        <property column="createdDate" name="createdDate" not-null="false"/>
        <property column="modifiedDate" name="modifiedDate" not-null="false"/>
        <property column="modifiedBy" name="modifiedBy" not-null="false"/>
 
    </class>
</hibernate-mapping>
