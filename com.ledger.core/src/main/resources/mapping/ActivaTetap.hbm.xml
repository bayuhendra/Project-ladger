<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class mutable="true" name="com.ledger.domain.financial.management.ActivaTetap" table="TXN_ACTIVA">
        <!--<cache usage="read-only"/>-->
        <id column="id" name="id">
            <generator class="org.hibernate.id.IdentityGenerator"/>
            <!--<generator class="increment"/>-->
        </id>
        <property column="activaTetapID" name="activaTetapID" not-null="true"/>
        <property column="userID" name="userID" not-null="false"/>
        <property column="namaActivaTetap" name="namaActivaTetap" not-null="false"/>
        <property column="hargaActivaTetap" name="hargaActivaTetap" not-null="false"/>
        <property column="lamaPemakaian" name="lamaPemakaian" not-null="false"/>
        <property column="jangkaWaktuPenyusutan" name="jangkaWaktuPenyusutan" not-null="false"/>
        <property column="persenPenyusutan" name="persenPenyusutan" not-null="false"/>
        <property column="totalPenyusutan" name="totalPenyusutan" not-null="false"/>
        <property column="status" name="status" not-null="false"/>
        <property column="createdBy" name="createdBy" not-null="false"/>
        <property column="createdDate" name="createdDate" not-null="false"/>
        <property column="modifiedDate" name="modifiedDate" not-null="false"/>
        <property column="modifiedBy" name="modifiedBy" not-null="false"/>
    </class>
</hibernate-mapping>
